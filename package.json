    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "axios": "^0.21.1",
    "canvas": "^2.7.0",
    "chalk": "^4.1.0",
    "chart.js": "^2.9.4",
    "chartjs-node-canvas": "^3.1.0",
    "discord.js": "^12.5.1",
    "discord.js-commando": "github:discordjs/Commando",
